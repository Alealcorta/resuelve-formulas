{
    "name": "alealcorta/resuelve-formulas",
    "description": "Recibe formulas a resolver y devuelve su resultado",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Alejandra Alcorta",
            "email": "alejandra.alcorta@gmail.com"
        }
    ],
    "minimum-stability": "stable",
